 [32mstruct[m test_struct *[30m[43mptr[m = ([32mstruct[m test_struct *)malloc([33msizeof[m([32mstruct[m test_struct));
 [33mif[m([30m[43mptr[m == [31mNULL[m){
  printf([31m"[m[35m\n[m[31mNode is not created[m[35m\n[m[31m"[m);
  [33mreturn[m [31mNULL[m;
 }

 [30m[43mptr[m->val = val;
 [30m[43mptr[m->next = [31mNULL[m;
 head = curr = [30m[43mptr[m
 [33mreturn[m [30m[43mptr[m;
}

[32mstruct[m test_struct* add_to_list([32mint[m val, [32mbool[m add_to_end){
 [33mif[m([31mNULL[m == head)
  [33mreturn[m (create_list(val));
 [33mif[m(add_to_end)
 printf([31m"[m[35m\n[m[31m Adding to the end of the list[m[35m\n[m[31m"[m);
 [33melse[m
 printf([31m"[m[35m\n[m[31m Adding to the start of the list[m[35m\n[m[31m"[m);[37;2H[33mreturn[m [30m[43mptr[m;
}
